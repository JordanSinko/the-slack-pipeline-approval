    "esbuild": "^0.7.9",
    "husky": "^4.3.0",
    "jest": "^26.4.2",
    "jsii": "^1.11.0",
    "jsii-diff": "^1.11.0",
    "jsii-docgen": "^1.3.2",
    "jsii-pacmak": "^1.11.0",
    "jsii-release": "^0.1.6",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.2",
    "pretty-quick": "^3.0.2",
    "projen": "^0.3.92",
